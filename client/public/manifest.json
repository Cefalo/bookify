{
  "name": "Quick Meet",
  "version": "1.0.2",
  "description": "One click to book them all",
  "manifest_version": 3,
  "author": "Ali Ahnaf",
  "action": {
    "default_popup": "index.html",
    "default_icon": "favicon-32x32.png",
    "default_title": "Quick Meet"
  },
  "host_permissions": [
    "http://localhost:3000/*"
  ],
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png",
    "128": "logo128.png"
  },
  "permissions": [
    "identity",
    "storage"
  ]
}