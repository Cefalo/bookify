{
  "name": "QuickMeet",
  "version": "1.0.0",
  "description": "One click to book them all",
  "manifest_version": 3,
  "author": "Ali Ahnaf",
  "action": {
    "default_popup": "index.html",
    "default_icon": "favicon-32x32.png",
    "default_title": "QuickMeet"
  },
  "host_permissions": [
    "https://quickmeet.cefalolab.com/*"
  ],
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png"
  },
  "permissions": [
    "identity",
    "storage"
  ]
}